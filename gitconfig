
[user]

[core]
	autocrlf = input
	safecrlf = warn
[alias]
  co = commit
  st = status
  ad = add
  pu = push
  pl = pull
hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	
# ignored by Git older than 1.7.10
[include]
	path = .gitconfig.local
